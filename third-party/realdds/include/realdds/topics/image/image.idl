module realdds
{
    module topics
    {
        module raw
        {
            module device
            {
                struct image
                {
                    sequence<octet> raw_data;
                    string frame_id; // String to match ros frame_id (see http://docs.ros.org/en/melodic/api/std_msgs/html/msg/Header.html)
                    unsigned long size;
                    unsigned short width;
                    unsigned short height;
                    octet format; // See librealsense 'rs2_format' type for values
                };
            };
        };
        
    };
};
